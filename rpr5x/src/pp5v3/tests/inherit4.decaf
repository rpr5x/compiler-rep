class Animal {
  void Method1() {
    Print("Animal");
  }
}

class Cow extends Animal {
   void Method1() {
    Print("Cow");
  }
}
class Jersey extends Cow {
   void Method1() {
      Print("Jersey");
   }
}

void main() {
  Animal a;

   a = New(Animal);
   a.Method1();
  a = New(Cow);
  a.Method1();
  a = New(Jersey);
  a.Method1();
}
